{
  "name": "pup-finance-server",
  "version": "1.0.0",
  "description": "TypeScript Express API for Pup Finance",
  "main": "dist/app.js",
  "scripts": {
    "dev": "npx ts-node-dev --respawn src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "dotenv -e .env.test -- jest --watchAll",
    "test:migrate": "dotenv -e .env.test -- prisma migrate deploy",
    "lint": "eslint . --ext .ts,.tsx",
    "prisma": "prisma",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^4.21.2",
    "express-oauth2-jwt-bearer": "^1.6.1",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prisma": "^6.10.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>/src"
    ]
  }
}
