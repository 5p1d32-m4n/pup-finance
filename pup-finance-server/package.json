{
  "name": "pup-finance-server",
  "version": "1.0.0",
  "description": "TypeScript Express API for Pup Finance",
  "main": "dist/app.js",
  "scripts": {
    "dev": "concurrently \"npx prisma generate\" \"ts-node-dev --respawn src/app.ts\"",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest --watchAll",
    "lint": "eslint . --ext .ts,.tsx",
    "prisma": "prisma",
    "migrate": "prisma migrate dev",
    "generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-oauth2-jwt-bearer": "^1.1.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prisma": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>/src"
    ]
  }
}
